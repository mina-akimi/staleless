{
  "name": "@mina-akimi/staleless",
  "version": "1.1.0",
  "description": "Zero dependency lib to solve stale state in React",
  "author": "Minasan Akimi",
  "repository": "https://github.com/mina-akimi/staleless",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "typescript",
    "stale",
    "staleless"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts",
    "fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "license": "MIT",
  "packageManager": "yarn@3.6.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2"
  }
}
